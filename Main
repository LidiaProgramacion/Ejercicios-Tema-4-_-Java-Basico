        System.out.println("La pantalla mide " + S1.getpantalla() + " pulgadas");
        System.out.println("Tiene cargador: " + S1.getCargador());

    Smartwatch S2 = new Smartwatch("Amazfit", "GTS 4",3000L, 1.6f, false);
        System.out.println("La marca es: " + S2.getmarca());
        System.out.println("El modelo es-> " + S2.getmodelo());
        System.out.println("Tiene " + S2.getbateria() + " AH de bateria");
        System.out.println("La pantalla tiene " + S2.getbateria() + " pulgadas");
