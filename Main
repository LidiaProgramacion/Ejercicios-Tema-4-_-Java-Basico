package EjerciciosTema4;

public class Main {
    public static void main(String[] args) {
    Smartphone S1 = new Smartphone("Samsung", "A 13", 8000L, 6.7f, true);

        System.out.println ("La Marca es: " + S1.getmarca());
        System.out.println("Modelo ->" + S1.getmodelo());
